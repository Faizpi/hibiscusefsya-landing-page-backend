{
  "name": "hibiscus-admin-dashboard",
  "version": "1.0.0",
  "description": "Admin Dashboard for Hibiscus Efsya Landing Page",
  "main": "server/index.js",
  "scripts": {
    "server": "nodemon server/index.js",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build --prefix client",
    "start": "node server/index.js",
    "setup-db": "node server/database/setup.js"
  },
  "keywords": ["admin", "dashboard", "hibiscus", "efsya"],
  "author": "Hibiscus Efsya",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "mysql2": "^3.16.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11"
  }
}
